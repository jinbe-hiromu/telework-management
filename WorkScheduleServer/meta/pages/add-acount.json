{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Id",
                              "component": "Id",
                              "name": "IdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "Id",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${acount.Id}"
                            },
                            {
                              "type": "required-validator",
                              "name": "IdRequiredValidator",
                              "component": "Id",
                              "text": "Id is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Name",
                              "component": "Name",
                              "name": "NameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "Name",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${acount.Name}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NameRequiredValidator",
                              "component": "Name",
                              "text": "Name is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Password",
                              "component": "Password",
                              "name": "PasswordLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "Password",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${acount.Password}"
                            },
                            {
                              "type": "required-validator",
                              "name": "PasswordRequiredValidator",
                              "component": "Password",
                              "text": "Password is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Notes",
                              "component": "Notes",
                              "name": "NotesLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "Notes",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${acount.Notes}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Acount!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "WorkScheduleDB.createAcount",
                        "parameters": [
                          {
                            "name": "Acount",
                            "value": "${acount}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${acount}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${acount}",
                  "visible": "${acount != null}",
                  "itemType": "WorkScheduleServer.Models.WorkScheduleDb.Acount"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "acount",
        "type": "set",
        "value": "new WorkScheduleServer.Models.WorkScheduleDb.Acount(){}",
        "valueType": "WorkScheduleServer.Models.WorkScheduleDb.Acount"
      }
    ]
  },
  "name": "Add Acount",
  "layout": "Main",
  "access": [
    "Authenticated"
  ]
}