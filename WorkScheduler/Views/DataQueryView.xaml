<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:vm="clr-namespace:WorkScheduler.ViewModels"
             x:Class="WorkScheduler.Views.DataQueryView"
             Title="WebViewContentPage">

    <ContentPage.BindingContext>
        <vm:DataQueryViewModel />
    </ContentPage.BindingContext>

    <WebView
        Source="{Binding Source, Mode=OneWay}"
        Cookies="{Binding Cookies, Mode=OneWay}" />

</ContentPage>