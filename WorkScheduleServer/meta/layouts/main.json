                    {
                      "events": {
                        "click": [
                          {
                            "code": "await InvokeAsync(() => { sidebar0.Toggle(); });",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { body0.Toggle(); });",
                            "type": "execute"
                          }
                        ]
                      },
                      "name": "sidebar-toggle0",
                      "type": "sidebar-toggle"
                    },
                    {
                      "name": "label0",
